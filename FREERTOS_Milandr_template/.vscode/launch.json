        {
            "name": "J-Link Debug",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "cwd": "${workspaceRoot}",
            // "executable": "build/mdr_test.elf",
            "executable": "${command:cmake.launchTargetPath}",
            "preLaunchTask": "CMake: build",
            "preRestartCommands": [
                "load",
                "enable breakpoint",
                "monitor reset"
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw",
            "device": "K1986BE92QI",
            "svdFile": "${workspaceRoot}/MDR32F9Q2I.svd",
        },
        {
            "name": "J-Link Attach",
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "jlink",
            "cwd": "${workspaceRoot}",
            // "executable": "build/mdr_test.elf",
            "executable": "${command:cmake.launchTargetPath}",
            "preLaunchTask": "CMake: build",
            "preRestartCommands": [
                "load",
                "enable breakpoint",
                "monitor reset"
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw",
            "device": "K1986BE92QI",
            "svdFile": "${workspaceRoot}/MDR32F9Q2I.svd",
        }
